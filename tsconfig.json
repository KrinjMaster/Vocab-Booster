{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"noEmit": true,
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"typeRoots": [
			"/src/app.d.ts",
		],
    "paths": {
      "$lib": [ "./src/lib/" ],
      "$lib/*": [ "./src/lib/*" ],
      "$src": [ "./src/" ],
      "$src/*": [ "./src/*" ],
    },
		"sourceMap": true,
		"importsNotUsedAsValues":"remove",
		"strict": true,
	},
}
